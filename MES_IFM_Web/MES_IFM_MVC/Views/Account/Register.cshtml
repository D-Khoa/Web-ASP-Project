@model MES_IFM_MVC.Models.Account.aa0001

@{
    ViewData["Title"] = "Register";
}

<h2>Register</h2>

<hr />
<div class="row">
    <div class="col-md-4">
        <form method="post">
            <div class="text-danger">@ViewBag.Message</div>
            <div class="form-group">
                <label asp-for="aa0001c11" class="control-label">Fist Name</label>
                <input asp-for="aa0001c11" class="form-control" />
            </div>
            <div class="form-group">
                <label asp-for="aa0001c12" class="control-label">Last Name</label>
                <input asp-for="aa0001c12" class="form-control" />
            </div>
            <div class="form-group">
                <label asp-for="aa0001c13" class="control-label">Email</label>
                <input asp-for="aa0001c13" class="form-control" />
            </div>
            <div class="form-group">
                <label asp-for="aa0001c14" class="control-label">Tel</label>
                <input asp-for="aa0001c14" class="form-control" />
            </div>
            <div class="form-group">
                <label asp-for="aa0001c21" class="control-label">Password</label>
                <input type="password" asp-for="aa0001c21" class="form-control" />
            </div>
            <div class="form-group">
                <label asp-for="aa0001c22" class="control-label">Confirm Password</label>
                <input type="password" asp-for="aa0001c22" class="form-control" />
            </div>
            <div class="form-group">
                <button type="submit" asp-action="/Register" asp-controller="Account" class="btn btn-success">Register</button>
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Login">Back to Login</a>
</div>

